{
  "rewrites": [
    {
      "source": "/images/:path*",
      "destination": "https://xrtfrtiubugctntwbami.supabase.co/storage/v1/object/public/products/:path*"
    },
    {
      "source": "/videos/:path*",
      "destination": "https://xrtfrtiubugctntwbami.supabase.co/storage/v1/object/public/products/videos/:path*"
    }
  ],
  "headers": [
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/videos/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    }
  ]
}
